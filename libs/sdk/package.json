{
  "name": "@api7/adc-sdk",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./schema": {
      "types": "./src/core/schema.ts",
      "import": "./src/core/schema.ts",
      "default": "./src/core/schema.ts"
    }
  },
  "nx": {
    "name": "sdk",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/sdk",
          "tsConfig": "libs/sdk/tsconfig.lib.json",
          "packageJson": "libs/sdk/package.json",
          "main": "libs/sdk/src/index.ts",
          "assets": [
            "libs/sdk/*.md"
          ]
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ]
      },
      "test": {
        "executor": "@nx/vite:test",
        "options": {
          "configFile": "libs/sdk/vitest.config.ts"
        }
      }
    }
  },
  "dependencies": {
    "axios": "^1.13.1",
    "deep-diff": "^1.0.2",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "semver": "^7.6.3",
    "tslib": "^2.6.2",
    "vitest": "3.0.0",
    "zod": "^4.0.10"
  }
}
