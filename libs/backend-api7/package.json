{
  "name": "@api7/adc-backend-api7",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "devDependencies": {
    "@api7/adc-sdk": "workspace:*",
    "@api7/adc-differ": "workspace:*"
  },
  "nx": {
    "name": "backend-api7",
    "projectType": "library",
    "targets": {
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ]
      },
      "test": {
        "executor": "@nx/vite:test",
        "options": {
          "configFile": "libs/backend-api7/vitest.config.ts"
        }
      }
    }
  }
}
